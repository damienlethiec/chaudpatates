<% unless @tickets_before_booking == 0 %>
	$("#training-<%= @training.id %>").replaceWith("<%= j render 'booked_training', training: @training %>");
  // Add avatar in the city members listing
  $(".members-listing").prepend("<%= j render 'users/user_card', city_member: @booking.user %>");
  $(".section-members ").removeClass('inactive');
  $(".navbar-wagon").replaceWith("<%= j render 'shared/navbar', user: current_user %>");
  // Remove one ticket from navbar counter
<% else %>
	$(".stripe-button-el").click();
<% end %>
