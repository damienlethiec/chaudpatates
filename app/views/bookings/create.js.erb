<% unless @tickets_before_booking == 0 %>
	$("#training-<%= @training.id %>").replaceWith("<%= j render 'booked_training', training: @training %>");
  // Add avatar in the city members listing
  $(".members-listing").prepend('
    <div class="member"><% if current_user.linkedin_picture_url %>
                      <%= link_to current_user.linkedin_profile, target: "_blank" do %>
                        <%= image_tag current_user.linkedin_picture_url, width: 80, height: 80, crop: :fill,  class: "avatar-big"%>
                      <% end %>
                    <% elsif current_user.photo %>
                      <%= cl_image_tag current_user.photo.path, width: 160, height: 160, crop: :fill, class: "avatar-big"%>
                    <% else %>
                      <%= image_tag "http://res.cloudinary.com/ind-pendant/image/upload/v1478766698/user_mkbsoj.png", width: 80, height: 80, crop: :fill, class: "avatar-big" %>
                    <% end %>
                    <p><%= current_user.first_name %></p></div>
    ');
  $(".section-members ").removeClass('inactive');
  // Remove one ticket from navbar counter
<% else %>
	$(".stripe-button-el").click();
<% end %>
